{
  "name": "quick-ops",
  "displayName": "quickOps",
  "description": "quick-ops",
  "version": "0.0.10",
  "engines": {
    "vscode": "^1.54.0"
  },
  "publisher": "quick-ops",
  "categories": [
    "Other"
  ],
  "icon": "./icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/WONISHI/quick-ops.git"
  },
  "activationEvents": [
    "onStartupFinished",
    "onCommand:actions.find",
    "workspaceContains:.logrc",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:vue",
    "onLanguage:html"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.revealCurrentFile",
        "title": "定位到当前文件",
        "category": "quickOps",
        "icon": {
          "light": "resources/svg/position-dark.svg",
          "dark": "resources/svg/position-light.svg"
        }
      },
      {
        "command": "myExtension.scrollToTop",
        "title": "回到顶部",
        "icon": {
          "light": "resources/svg/top-dark.svg",
          "dark": "resources/svg/top-light.svg"
        }
      },
      {
        "command": "myExtension.scrollToBottom",
        "title": "回到底部",
        "icon": {
          "light": "resources/svg/bottom-dark.svg",
          "dark": "resources/svg/bottom-light.svg"
        }
      },
      {
        "command": "extension.createLogrcFile",
        "title": "创建 .logrc 文件"
      },
      {
        "command": "extension.CopyTsType",
        "title": "复制 TypeScript 类型"
      },
      {
        "command": "extension.MockData",
        "title": "模拟数据生成"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "extension.revealCurrentFile",
          "when": "resourceLangId != ''",
          "group": "navigation"
        },
        {
          "command": "myExtension.scrollToTop",
          "group": "navigation",
          "when": "editorTextFocus"
        },
        {
          "command": "myExtension.scrollToBottom",
          "group": "navigation",
          "when": "editorTextFocus"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.createLogrcFile",
          "group": "navigation@1",
          "when": "Extension.logrcNotFound"
        }
      ],
      "editor/context": [
        {
          "command": "extension.CopyTsType",
          "group": "navigation@1",
          "when": "Extension.SelectTots"
        },
        {
          "command": "extension.MockData",
          "group": "navigation@2",
          "when": "Extension.SelectToMock"
        }
      ]
    }
  },
  "scripts": {
    "build": "npx vsce package",
    "vscode:prepublish": "pnpm run package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir dist",
    "watch-tests": "tsc -p . -w --outDir dist",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "pug": "^3.0.3",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "volta": {
    "node": "22.18.0"
  },
  "dependencies": {
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vue/compiler-sfc": "^3.5.20",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.13",
    "ignore": "^7.0.5",
    "mockjs": "^1.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "terser-webpack-plugin": "^5.3.14",
    "vscode-languageclient": "^9.0.1",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
