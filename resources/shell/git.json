[
    {
        "description": "查看当前文件状态",
        "cmd": "git status"
    },
    {
        "description": "查看所有分支（包含远程）",
        "cmd": "git branch -a"
    },
    {
        "description": "暂存当前修改",
        "cmd": "git stash"
    },
    {
        "description": "恢复最近一次暂存的修改",
        "cmd": "git stash pop"
    },
    {
        "description": "撤销提交",
        "cmd": "git reset --soft HEAD~1"
    }
]