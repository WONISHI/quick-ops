      "function debounce(() => {",
      "   let timer = null",
      "   return (callback, wait = 800) => {",
      "       timer&&clearTimeout(timer)",
      "       timer = setTimeout(callback, wait)",
      "   }",
      "})()"
    ],
    "style": "javascript",
    "description": "防抖 (quick-ops提供)"
  },
  {
    "prefix": "thr",
    "body": [
      "function throttle(() => {",
      "   let last = 0",
      "   return (callback, wait = 800) => {",
      "       let now = +new Date()",
      "       if (now - last > wait) {",
      "           callback()",
      "           last = now",
      "       }",
      "   }",
      "})()"
    ],
    "style": "javascript",
    "description": "节流 (quick-ops提供)"
  },
