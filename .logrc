{
  "general": {
    "debug": true,
    "excludeConfigFiles": false
  },
  "logger": {
    "template": "[icon]-[~/^/^/name]-[line]-[$0]",
    "dateFormat": "YYYY-MM-DD HH:mm:ss"
  },
  "utils": {
    "uuidLength": 12
  },
  "mock": {
    "enabled": true,
    "asyncMode": true,
    "port": 3100,
    "workerCount": 5
  },
  "git": {
    "ignoreList": []
  },
  "project": {
    "marks": {
      "@FIXME": {
        "backgroundColor": "#9c27b0",
        "color": "#fff",
        "borderRadius": "2px"
      },
      "@OPTIMIZE": {
        "backgroundColor": "#00bcd4",
        "color": "#fff"
      }
    },
    "alias": {
      "@/": "./src/"
    }
  }
}